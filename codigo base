    <script>


        $(function () {
            var $login = $('.navbar .login');

            var cookieArray = document.cookie.split(";");

            var isLogged = cookieArray.findIndex(x => x.indexOf("CineToken") > -1) > -1;



            var sessionUserName = $("#hdnSessionUserName").data('value');

            if (sessionUserName === ''|| typeof sessionUserName === 'undefined') {
                sessionUserName = 'Minha Conta';
            }

            if(isLogged) {
                $login.html([
                            '<a class="btn" href="https:/intercine.com.br//inter-cine/pagina-inicial">',
                            '<span class="icon icon-user"></span><span class="label">' + sessionUserName +'</span>',
                            '</a>'
                ].join(''));
            }

            

        });


    </script>

    

